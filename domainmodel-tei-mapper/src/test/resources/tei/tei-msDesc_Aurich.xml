                <msContents>
                    <msItem xml:id="obj_90411540_INI">
                        <note type="text">
                        
                            
                            
                            <locus>1r</locus>
                        
                            
                            
                            <index indexName="initien">
                                <term facs="#HSK0440_a032" xml:lang="de">God vater herr Jesu Christ/ dein genad unzergäncklich ist</term>
                            </index>
                     </note>
                    </msItem>
                    <msItem xml:id="obj_90411544_INI">
                        <note type="text">
                        
                            
                            
                            <locus>127r</locus>
                        
                            
                            
                            <index indexName="initien">
                                <term facs="#HSK0440_a032" xml:lang="de">Ein stat hies Troya do was ain kunig inne der was gewaltig</term>
                            </index>
                     </note>
                    </msItem>
                    <msItem xml:id="obj_90411546_INI">
                        <note type="text">
                        
                            
                            
                            <locus>223r</locus>
                        
                            
                            
                            <index indexName="initien">
                                <term facs="#HSK0440_a032" xml:lang="de">Sovil die sunne liechter ist dann ander stern</term>
                            </index>
                     </note>
                    </msItem>
                    <msItem xml:id="obj_90411549_INI">
                        <note type="text">
                        
                            
                            
                            <locus>225r</locus>
                        
                            
                            
                            <index indexName="initien">
                                <term facs="#HSK0440_a032" xml:lang="de">Iohannes der ausgezogen hat diß puch, ain sun Patricii</term>
                            </index>
                     </note>
                    </msItem>
                    <msItem xml:id="obj_90411550_INI">
                        <note type="text">
                        
                            
                            
                            <locus>225v</locus>
                        
                            
                            
                            <index indexName="initien">
                                <term facs="#HSK0440_a032" xml:lang="de">Aller erenreichster sun, aller gerechtister keyser, got wölle dich bestercken</term>
                            </index>
                     </note>
                    </msItem>
                    <msItem xml:id="obj_90411551_INI">
                        <note type="text">
                        
                            
                            
                            <locus/>
                        
                            
                            
                            <index indexName="initien">
                                <term facs="#HSK0440_a032" xml:lang="de">Das schönest wappen acht ich fein</term>
                            </index>
                     </note>
                    </msItem>
