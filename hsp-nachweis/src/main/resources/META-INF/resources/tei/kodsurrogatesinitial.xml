<TEI xmlns="http://www.tei-c.org/ns/1.0">
   <teiHeader>
      <fileDesc>
         <titleStmt>
            <title>Surrogates</title>
         </titleStmt>
         <publicationStmt>
            <p>Publication Information</p>
         </publicationStmt>
         <sourceDesc>
            <msDesc>
               <msIdentifier>
                  <repository>.</repository>
               </msIdentifier>
               <additional>
                  <adminInfo/>
                  <surrogates>
                     <!--  include digitized objects
                       # required: insert KOD-ID or valid idno
                       # required: link to an 
                       ## IIIF-Manifest -> type="manifest" if digitized object is capable of beeing depicted in an IIIF- viewer
                       ## OR any other digitized depiction -> type="other"
                       # recommended: insert type="thumbnail" for depicting a selected preview
                       # recommended: insert date of digitalization, place and name of the organization that issued the digitized object
                     -->
                     <bibl>
                        <ref type="hsp:object" target="__UUID_des_KODs_oder_gueltige_Signatur(idno)"/>
                        <ref type="manifest" subtype="completeFromOriginal" target="___URL_des_Manifestes___"/>
                        <ref type="other" subtype="completeFromOriginal" target="___URL_des_Digitalisates___"/>
                        <ref type="thumbnail" target="___URL_des_Thumbnails___"/>
                        <date when="2020-10-06" type="digitized">2020-10-06</date>
                        <date when="2020-10-07" type="issued">2020-10-07</date>
                        <orgName ref="gnd-ID_der_Einrichtung">___digitalisierendeEinrichtung___</orgName>
                        <placeName ref="gnd-ID_der_Einrichtung">___OrtDerDigitalisierendenEinrichtung___</placeName>
                     </bibl>
                  </surrogates>
               </additional>
            </msDesc>
         </sourceDesc>
      </fileDesc>
   </teiHeader>
